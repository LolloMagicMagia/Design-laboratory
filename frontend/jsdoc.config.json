{
  "source": {
    "include": ["src/app"],
    "exclude": ["**/node_modules/**"]
  },
  "opts": {
    "destination": "docs",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true
    }
  }
}